    "graphology": "^0.23.1",
    "http-errors": "~1.6.3",
    "jade": "^0.31.2",
    "morgan": "~1.9.1",
    "neo4j-driver": "^4.4.1"
  },
  "devDependencies": {
    "sigma": "^1.2.1"
  }
}
